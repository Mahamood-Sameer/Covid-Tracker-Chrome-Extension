{
    "manifest_version": 2,
    "name": "Covid Tracker-India",
    "version": "1.0.0",
    "description": "Tracking the Number of Active cases,Recoveries,Deaths,Confirmed cases ",
    "icons": {"128":"icon_128.png"},
    "browser_action" : {
        "default_icon":"icon.png",
        "default_popup" : "index.html"
    },
    "permissions" : ["https://api.covid19india.org/data.json"]
}